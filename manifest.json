{
  "name": "Github review requests list",
  "version": "0.0.7",
  "description": "Github review requests list",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": ["js/onInstall.js"],
    "persistent": false
  },
  "permissions": [ "storage", "notifications" ],
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "48": "img/icon_64.png",
    "128": "img/icon_128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
