{
  "name": "Github review requests list",
  "version": "0.0.18",
  "description": "Github review requests list",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["js/namespace.js", "js/notifications.js", "js/github.js", "js/badge.js", "js/background.js"]
  },
  "permissions": [ "storage", "notifications", "alarms", "activeTab", "idle", "contextMenus"  ],
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "48": "img/icon_64.png",
    "128": "img/icon_128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon_128.png",
      "38": "img/icon_128.png"
    },
    "default_title": "Github review requests list",
    "default_popup": "popup.html"
  }
}
